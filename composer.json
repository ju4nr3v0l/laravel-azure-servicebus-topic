{
    "name": "ju4nr3v0l/laravel-azure-servicebus",
    "description": "Extending the Laravel queue to support the Microsoft Azure Service Bus Queues & topics for php 7.4",
    "version": "8.0.0",
    "type": "library",
    "keywords": [
        "laravel",
        "azure",
        "servicebus",
        "queues"
    ],
    "license": "MIT",
    "authors": [{
        "name": "ju4nr3v0l",
        "email": "jadamavecool@gmail.com"
    }],
    "require": {
        "php": ">=7.1.10",
        "illuminate/container": ">=8.0.0 <9.0.0",
        "illuminate/support": ">=8.0.0 <9.0.0",
        "illuminate/encryption": ">=8.0.0 <9.0.0",
        "illuminate/queue": ">=8.0.0 <9.0.0",
        "microsoft/windowsazure": "*"
    },
    "autoload": {
        "psr-4": {
            "Ju4nr3v0l\\LaravelAzureServicebus\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ju4nr3v0l\\LaravelAzureServicebus\\Support\\ServiceProvider"
            ]
        }
    }
}